    "ionicons": "^7.0.0",
    "pinia": "^2.1.0",
    "typescript": "~5.6.2",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "vue-tsc": "^2.1.10"
  },
  "devDependencies": {
    "@capacitor/cli": "7.2.0",
    "@catch-coin/frontend-tests": "workspace:*",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint-plugin-vue": "^9.9.0",
